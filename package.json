{
  "name": "gatsby-starter-shopify-app",
  "private": true,
  "description": "A GatsbyJS start to create a Shopify admin app",
  "version": "1.0.0",
  "author": "Gil Greenberg <gilgreenberg@gmail.com>",
  "dependencies": {
    "@shopify/polaris": "^3.3.0",
    "dotenv": "^6.2.0",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-netlify-functions": "^0.0.3",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "netlify-lambda": "^1.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "shopify-token": "^3.0.3",
    "universal-cookie": "^3.0.7"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "serverless-shopify",
    "shopify-app",
    "gatsby-shopify"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "build-server": "netlify-lambda build functions/src",
    "start-server": "netlify-lambda serve functions/src -p 7000"
